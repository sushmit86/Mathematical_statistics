1.0*(lead_data.leads - LAG(lead_data.leads,28) over (order by user_session_data.day ))/NVL(LAG(lead_data.leads,28) over (order by user_session_data.day ),0) as "%_28_leads_change",
LAG(lead_data.leads,364) over (order by user_session_data.day ) as "364_leads",
1.0*(lead_data.leads - LAG(lead_data.leads,364) over (order by user_session_data.day ))/NVL(LAG(lead_data.leads,364) over (order by user_session_data.day ),0) as "%_364_leads_change"
from
(
select
date_trunc('day',session_start_time)::date as day,
count( distinct mixpanel_id) as uv,
count(  mixpanel_id) as sessions
from analytics_mv.mv_mx_classified_session2 t1
left join analytics_mv.mv_reporting_marketing_medium_mapping t2
on t1.medium = t2.marketing_medium
--where date_trunc('day',session_start_time)::date = current_date
group by 1
order by 1 desc,2) user_session_data
left join
(
select
date_trunc('day',created_at)::date as day,
count(distinct email) as leads
from
analytics_mv.mv_fact_conversion_all_touches
group by 1
order by 1 desc) lead_data
on user_session_data.day = lead_data.day
order by 1 desc")
